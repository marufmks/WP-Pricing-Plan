{
    "name": "mkswp/pricing-plan",
    "description": "Pricing Plan is a plugin for WordPress that allows you to create and manage pricing plans for your products and services.",
    "type": "project",
    "license": "GPLv2",
    "authors": [
        {
            "name": "Maruf Khan",
            "email": "marufkh204@gmail.com"
        }
    ],
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.1.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1.6"
    },
    "scripts": {
        "lint":"phpcs",
        "lint:fix":"phpcbf"
    },
    "config": {
        "vendor-dir": "lib",
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {"Pricing_Plan\\": "./core"}
    }
}
